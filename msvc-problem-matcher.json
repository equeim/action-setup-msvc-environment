{
    "problemMatcher": [
        {
            "owner": "msvc-problem-matcher",
            "pattern": [
                {
                    "regexp": "^(?:\\s+\\d+>)?(\\S.*)\\((\\d+),?(\\d*)\\)\\s*:\\s+(error|warning|info)\\s+(\\w+\\d+)\\s*:\\s*(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "code": 5,
                    "message": 6
                }
            ]
        }
    ]
}
